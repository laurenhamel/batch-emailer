{
  "paths": {
    "root": "/",
    "data": {
      "root": "data/"
    },
    "src": {
      "root": "src/",
      "js": "src/js/",
      "scss": "src/scss/",
      "php": "src/php/",
      "fonts": "src/fonts/",
      "images": "src/images/",
      "partials": "src/partials/",
      "includes": "src/includes/"
    },
    "dev": {
      "root": "dev/",
      "js": "dev/js/",
      "css": "dev/css/",
      "php": "dev/php/",
      "fonts": "dev/fonts/",
      "images": "dev/images/",
      "includes": "dev/includes/",
      "dependencies": {
        "css": "dev/css/dependencies/",
        "js": "dev/js/dependencies/",
        "php": "dev/php/dependencies/"
      }
    },
    "dist": {
      "root": "dist/",
      "js": "dist/js/",
      "css": "dist/css/",
      "php": "dist/php/",
      "fonts": "dist/fonts/",
      "images": "dist/images/",
      "includes": "dist/includes/",
      "dependencies": {
        "css": "dist/css/dependencies/",
        "js": "dist/js/dependencies/",
        "php": "dist/php/dependencies/"
      }
    },
    "composer": {
      "root": "vendor/"
    }
  },
  "css": [
    "style.css"
  ],
  "js": [
    "<%= pkg.main %>"
  ],
  "dependencies": {
    "css": [
      "codemirror"
    ],
    "js": [
      "<%= pkg.dependencies %>",
      "codemirror/addon/overlay.js",
      "codemirror/addon/runmode.js"
    ],
    "php": [
      "phpmailer/phpmailer",
      "mustache/mustache",
      "vlucas/phpdotenv"
    ]
  }
}